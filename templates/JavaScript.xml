<templateSet group="JavaScript">
  <template name="jqready" value="$(document).ready(function () {&#10;            &#10;});" description="JQuery document ready function" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="timeout" value="setTimeout(() =&gt; {&#10; &#10; }, $duration$);" description="set a timeout" toReformat="false" toShortenFQNames="true">
    <variable name="duration" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="comment" value="/***********************************************************&#10; $title$&#10; --------------------------&#10; $desc$&#10; ************************************************************/&#10; $END$" description="Inserts a js comment" toReformat="false" toShortenFQNames="true">
    <variable name="title" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="desc" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>